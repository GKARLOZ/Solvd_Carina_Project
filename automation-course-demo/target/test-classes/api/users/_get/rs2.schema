{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "created_time": {
            "type": "string",
            "format": "date-time"
          },
          "message": {
            "type": "string"
          },
          "id": {
            "type": "string"
          }
        },
        "required": ["created_time", "message", "id"],
        "additionalProperties": false
      }
    },
    "paging": {
      "type": "object",
      "properties": {
        "previous": {
          "type": "string",
          "format": "uri"
        },
        "next": {
          "type": "string",
          "format": "uri"
        }
      },
      "additionalProperties": false
    }
  },
  "required": ["data", "paging"],
  "additionalProperties": false
}